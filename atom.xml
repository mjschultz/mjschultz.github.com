---
layout: nil
---
<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">
	<title>{{ site.title }} | Michael J. Schultz</title>
	<link href="http://blog.beyond-syntax.com/atom.xml" rel="self" />
	<link href="http://blog.beyond-syntax.com/" />
	<updated>{{ site.time | date_to_xmlschema }}</updated>
	<id>http://blog.beyond-syntax.com/</id>
	<author>
		<name>Michael J. Schultz</name>
		<email>mjschultz@gmail.com</email>
	</author>
	{% for post in site.posts %}
	<entry>
		<title>{{ post.title }}</title>
		<link href="http://blog.beyond-syntax.com{{ post.url }}" />
		<updated>{{ post.date | date_to_xmlschema }}</updated>
		<id>http://blog.beyond-syntax.com{{ post.id }}</id>
		<content type="html">{{ post.content | xml_escape }}</content>
	</entry>
	{% endfor %}
</feed>
